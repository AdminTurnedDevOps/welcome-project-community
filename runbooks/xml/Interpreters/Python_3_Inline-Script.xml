<joblist>
  <job>
    <context>
      <options preserveOrder='true'>
        <option name='name' value='a devops specialist'>
          <label>Title name</label>
        </option>
        <option name='years' value='12' />
      </options>
    </context>
    <defaultTab>nodes</defaultTab>
    <description><![CDATA[Basic Python 3 inline-script example:

More info here: https://docs.rundeck.com/docs/manual/node-steps/builtin.html#script-step]]></description>
    <executionEnabled>true</executionEnabled>
    <group>Basic Examples/Interpreters</group>
    <id>c38eb6ca-37df-4568-8261-05547564daee</id>
    <loglevel>INFO</loglevel>
    <name>Python 3 Inline-Script</name>
    <nodeFilterEditable>false</nodeFilterEditable>
    <plugins />
    <scheduleEnabled>true</scheduleEnabled>
    <sequence keepgoing='false' strategy='node-first'>
      <command>
        <description>Get the values from options and print it using a python 3 inline-script</description>
        <fileExtension>.py</fileExtension>
        <script><![CDATA[title = "@option.name@"
years = @option.years@
print("I have been {} for {} years ".format(title, years))]]></script>
        <scriptargs />
        <scriptinterpreter>/usr/bin/python3.8</scriptinterpreter>
      </command>
    </sequence>
    <uuid>c38eb6ca-37df-4568-8261-05547564daee</uuid>
  </job>
</joblist>