<joblist>
  <job>
    <context>
      <options preserveOrder='true'>
        <option name='name' value='a devops specialist'>
          <label>Title name</label>
        </option>
        <option name='years' value='12'>
          <label>Years</label>
        </option>
      </options>
    </context>
    <defaultTab>nodes</defaultTab>
    <description><![CDATA[Basic bash inline-script usage.

More info here: https://docs.rundeck.com/docs/manual/node-steps/builtin.html#script-step]]></description>
    <executionEnabled>true</executionEnabled>
    <group>Basic Examples/Interpreters</group>
    <id>cc910bb3-9672-4038-b1e1-db4ec6d32e59</id>
    <loglevel>INFO</loglevel>
    <name>Bash Inline-Script</name>
    <nodeFilterEditable>false</nodeFilterEditable>
    <plugins />
    <scheduleEnabled>true</scheduleEnabled>
    <sequence keepgoing='false' strategy='node-first'>
      <command>
        <description>Get the values from options to print it on bash inline-script</description>
        <fileExtension>.sh</fileExtension>
        <script><![CDATA[title="@option.name@"
years=@option.years@
echo "I have been $title for $years years"]]></script>
        <scriptargs />
        <scriptinterpreter>/bin/bash</scriptinterpreter>
      </command>
    </sequence>
    <uuid>cc910bb3-9672-4038-b1e1-db4ec6d32e59</uuid>
  </job>
</joblist>